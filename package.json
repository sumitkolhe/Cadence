{
  "name": "cadence",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "start": "node -C production .output/server/index.mjs",
    "lint": "eslint . --ext \".ts,.js,.vue,.json\" --fix",
    "format": "prettier --write \"src/**/*.{js,ts,json,vue}\"",
    "postinstall": "npx simple-git-hooks && nuxt prepare"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint && yarn format",
    "commit-msg": "yarn commitlint --edit $1"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.5",
    "@pinia/nuxt": "^0.4.6",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@kolhe/eslint-config": "^1.1.8",
    "@nuxtjs/tailwindcss": "^6.1.3",
    "@tailwindcss/forms": "^0.5.3",
    "@vueuse/core": "^9.6.0",
    "@vueuse/integrations": "^9.6.0",
    "@vueuse/nuxt": "^9.6.0",
    "eslint": "^8.29.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nuxt": "^3.0.0",
    "nuxt-icon": "^0.1.8",
    "prettier": "^2.8.1",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^4.9.4"
  }
}
